{
  "name": "rollup-plugin-babili",
  "version": "3.1.0",
  "description": "Simple rollup plugin for minifying code using Babili.",
  "main": "dist/rollup-plugin-babili.js",
  "jsnext:main": "dist/rollup-plugin-babili.es2015.js",
  "module": "dist/rollup-plugin-babili.es2015.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rlb",
    "lint": "eslint src/**/*.js tests/*.js",
    "pretest": "npm run lint && npm run build",
    "test": "mocha tests/*.js",
    "preversion": "npm test && git add -f dist/",
    "postversion": "git rm -r --cached dist/ && git commit -m \"chore(dist): clean after release [ci skip]\" && git push origin && git push origin --tags",
    "prepublish": "in-publish && npm run build || exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Comandeer/rollup-plugin-babili.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "rollup",
    "rollup-plugin",
    "babili",
    "minify"
  ],
  "author": "Comandeer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Comandeer/rollup-plugin-babili/issues"
  },
  "homepage": "https://github.com/Comandeer/rollup-plugin-babili#readme",
  "peerDependencies": {
    "rollup": ">=0.41.5"
  },
  "devDependencies": {
    "@comandeer/rollup-lib-bundler": "^0.1.1",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "chai": "^4.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.0.1",
    "in-publish": "^2.0.0",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.2",
    "rollup": "^0.42.0",
    "sourcemap-validator": "^1.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@comandeer/babel-plugin-banner": "^1.0.0",
    "babel-core": "^6.21.0",
    "babel-preset-babili": "^0.1.2"
  }
}
